{
  "framework": "vite",
  "rewrites": [
    {
      "source": "/redirect/:id",
      "destination": "/api/redirect/:id"
    },
    {
      "source": "/((?!api/|assets/).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "outputDirectory": "dist"
} 