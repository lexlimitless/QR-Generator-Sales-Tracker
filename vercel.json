{
  "rewrites": [
    {
      "source": "/redirect/:id",
      "destination": "/api/redirect/:id"
    },
    {
      "source": "/api/oauth/calendly/callback",
      "destination": "/api/oauth/calendly/callback"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "outputDirectory": "dist",
  "framework": "vite"
} 